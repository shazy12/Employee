<odoo>
    <template id="report_custom_card">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="record">
                <h2> Custom Report </h2>
            </t>
        </t>
    </template>

    <template id="report_crm_custom">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="record">
                <t t-call="hr_employee.report_custom_card"/>
            </t>
        </t>
    </template>
</odoo>
